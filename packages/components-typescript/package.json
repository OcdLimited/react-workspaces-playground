		"@testing-library/user-event": "^8.0.3",
		"@types/jest": "^24.0.25",
		"@types/node": "^13.1.6",
		"@types/react": "^16.9.17",
		"@types/react-dom": "^16.9.4",
		"@typescript-eslint/eslint-plugin": "^2.15.0",
