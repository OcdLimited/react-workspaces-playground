	},
	"scripts": {
		"storybook": "start-storybook -p 9010 -s public",
		"build-storybook": "build-storybook -s public"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"jest": {
		"collectCoverageFrom": [
			"src/**/*.{ts,tsx}",
			"!src/setupTests.{ts,tsx}",
			"!src/index.{ts,tsx}",
			"!src/serviceWorker.{ts,tsx}",
			"!src/**/*.(spec|test|stories).{ts,tsx}"
		],
		"coverageThreshold": {
			"global": {
				"statements": 80,
				"branches": 45,
				"functions": 80,
				"lines": 80
			}
		}
	}
}
