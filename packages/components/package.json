{
	"name": "@ocdlimited/components",
	"version": "0.0.2-alpha.0",
	"homepage": "https://ocdlimited.github.io/react-workspaces-playground/",
	"main": "src/index.js",
	"main:src": "src/index.js",
	"scripts": {
		"test": "react-scripts test",
		"lint": "eslint ./src/**/*.js --max-warnings=0 --format=codeframe"
	},
	"dependencies": {
		"@ocdlimited/react-scripts": "^3.4.1",
		"@storybook/react": "^5.3.18",
		"react": "^16.13.1",
		"react-dom": "^16.13.1"
	},
	"devDependencies": {
		"cross-env": "7.0.2",
		"enzyme": "^3.11.0",
		"enzyme-adapter-react-16": "^1.15.2",
		"eslint-config-prettier": "^6.9.0",
		"eslint-config-react-app": "^5.1.0",
		"eslint-plugin-prettier": "^3.1.2",
		"prop-types": "^15.7.2"
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"jest": {
		"collectCoverageFrom": [
			"src/**/*.{js}",
			"!src/setupTests.{js}",
			"!src/serviceWorker.{js}",
			"!src/**/*.(spec|test|stories).{js}"
		],
		"coverageThreshold": {
			"global": {
				"statements": 98,
				"branches": 98,
				"functions": 98,
				"lines": 98
			}
		}
	},
	"license": "MIT"
}
